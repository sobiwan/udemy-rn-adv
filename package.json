{
  "dependencies": {
    "prettier": "1.15.3"
  },
  "devDependencies": {
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --single-quote all --write --print-width 60",
      "git add"
    ]
  }
}
